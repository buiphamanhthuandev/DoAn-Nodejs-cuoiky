<div class="container">
    <h4><a href="/themdanhmuc" class="btn btn-primary">Thêm danh mục bài viết</a></h4>
    <table class="table table-hover table-striped">
        <thead>
          <tr>
            <th scope="col">ID</th>
            <th scope="col">Tên chủ đề</th>
            <th scope="col">Mô tả chủ đề</th>
            <th scope="col">EDIT</th>
            <th scope="col">DELETE</th>
          </tr>
        </thead>
        <tbody>
            <% resultAllCategory.forEach((item)=> { %>
                <tr>
                    
                    <th scope="row"><%=item.id%></th>
                    <td><%=item.TenDanhMuc%></td>
                    <td><%=item.MoTaDanhMuc%></td>
                    <td><a href="/suadanhmuc/<%=item.id%>" class="btn btn-warning">EDIT</a></td>
                    <td><a href="/xoadanhmuc/<%=item.id%>" class="btn btn-danger">DELETE</a></td>
                </tr>
                <%})%>
        </tbody>
      </table>
</div>